<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="0">

        <record id="group_gld_manager" model="res.groups">
            <field name="name">系统管理员</field>
            <field name="comment">系统管理员</field>
            <field name="implied_ids" eval=""/>
        </record>
        <record id="group_gld_manager" model="res.groups">
            <field name="name">工联单管理</field>
            <field name="comment">工联单管理</field>
            <field name="implied_ids" eval=""/>
        </record>
    </data>

    <data>
        <!--工联单 可以访问本公司的-->
        <record id="group_gld_manager_rule" model="ir.rule">
            <field name="name">访问自己公司的工联单</field>
            <field name="model_id" ref="model_gld"/>
            <field name="groups" eval="[(4, ref('group_gld_manager'))]"/>
            <field name="domain_force">[(1,'=',1)]
            </field>
        </record>
    </data>
</openerp>
