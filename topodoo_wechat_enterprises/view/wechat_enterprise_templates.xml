                <head>
                    <meta name="viewport"
                          content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no"/>
                    <t t-raw="head or ''"/>
                </head>
                <body>
                    <t t-raw="0"/>
                </body>
            </html>
        </template>

        <template id="topodoo_wechat_enterprises.Transfer" name="Wechat_Enterprise_Transfer">
            <t t-call="topodoo_wechat_enterprises.layout">
                <t t-set="head">
                    
                </t>
                <p id="url" style="display:none">
                    <t t-raw="url" />
                </p>
                <script src="/topodoo_wechat_enterprises/static/js/url_transfers.js"></script>
            </t>
        </template>

        <template id="topodoo_wechat_enterprises.wechat_warning" name="Wechat_Enterprise_Ｗarning">
            <t t-call="topodoo_wechat_enterprises.layout">
                <t t-set="head">
                </t>
                <div class="mr_center">
                    <p><h3>［<t t-esc="title"/>］</h3></p>
                    <p><t t-esc="content" /></p>
                </div>
            </t>
        </template>
    </data>
</openerp>